#include <iostream>
#include <vector>

// Функция для вычисления среднего значения чисел в векторе
double calculateAverage(const std::vector<int>& numbers) {
    int sum = 0;
    for (int num : numbers) {
        sum += num;
    }
    return static_cast<double>(sum) / numbers.size();
}

int main() {
    std::vector<int> numbers = {5, 10, 15, 20, 25};
    
    double average = calculateAverage(numbers);
    
    std::cout << "Среднее значение чисел: " << average << std::endl;
    
    return 0;
}
